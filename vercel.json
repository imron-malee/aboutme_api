{
    "version": 2,
    "builds":[
        {
            "src" :"index.js",
            "use" : "@now/node"
        },
        {
            "src" :"server.js",
            "use" : "@now/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/api/v2/(.*)",
            "dest": "index.js"
        }
    ]
}